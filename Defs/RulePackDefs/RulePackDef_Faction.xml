<?xml version="1.0" encoding="utf-8"?>
<Defs>
	<RulePackDef>
		<defName>CFAR_FactionNamerEmpire</defName>
		<rulePack>
			<rulesStrings>
				<li>r_name->Arellian Empire</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>

	<RulePackDef>
		<defName>CFAR_LeaderTitleMakerEmpire</defName>
		<rulePack>
			<rulesStrings>
				<li>r_leaderTitle->Grand Admiral</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>

	<RulePackDef>
		<defName>CFAR_NamerIdeo</defName>
		<rulePack><rulesStrings>
			<li>r_ideoName->The Imperial Codex</li><!--제국 율서-->

			<li>r_ideoAdjective->imperial</li>
			<li>r_ideoAdjective->honorable</li>

			<li>r_memberName->imperial</li>

			<li>creed(priority=1)->[memeCreed]</li>

			<li>hyphenPrefix->[memeHyphenPrefix]</li>
		</rulesStrings></rulePack>
	</RulePackDef>

	<RulePackDef>
		<defName>CFAR_NamerDeity</defName>
		<include>
			<li>CFAR_NamerUtility</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>r_name->[Name_M] [Surname_N]</li>
				<li>r_name->[Name_F] [Surname_N]</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>

	<RulePackDef>
		<defName>CFAR_DeityTypeMaker</defName>
		<rulePack>
			<rulesStrings>
				<li>r_deityType->[Deity]</li>

				<li>Deity->Legend</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>
	
<!--================================ Enclave ================================-->

	<RulePackDef>
		<defName>CFAR_FactionNamerPirate</defName>
		<rulePack>
			<rulesStrings>
				<li>r_name->Arellian Enclave</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>
</Defs>

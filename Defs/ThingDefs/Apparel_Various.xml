			<wornGraphicPath>Things/Pawn/Arellian/Apparel/TrooperArmor/Helmet/Helmet</wornGraphicPath>
			<layers>
				<li>Overhead</li>
			</layers>
			<tags>
				<li>CFAR_ArmorIndustrial</li>
			</tags>
			<defaultOutfitTags>
				<li>Soldier</li>
			</defaultOutfitTags>
		</apparel>
		<colorGenerator Class="ColorGenerator_Options">
			<options>
				<li><only>(1,1,1)</only></li>
			</options>
		</colorGenerator>
		<tradeTags>
			<li>CFAR_Armor_Fed</li>
		</tradeTags>
	</ThingDef>


<?xml version="1.0" encoding="utf-8"?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>[NL] Facial Animation - WIP</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAdd">
					<xpath>Defs</xpath>
					<value>
						<FacialAnimation.EyeballColorDef>
							<defName>Eyes_Arellian_EyeColor</defName>
							<geneDef>Eyes_Arellian</geneDef>
							<eyeballColor>RGB(255,212,83)</eyeballColor>
						</FacialAnimation.EyeballColorDef>
					</value>
				</li>
				<li Class="PatchOperationRemove">
					<xpath>Defs/GeneDef[defName="Eyes_Arellian"]/renderNodeProperties</xpath>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>